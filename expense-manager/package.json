{
    "name": "expense-manager",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -o --port 4200",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "pretty-project-check": "prettier --check \"**/*.{ts,js,json,html,scss,css}\"",
        "pretty-project-write": "prettier --write \"**/*.{ts,js,json,html,scss,css}\"",
        "pretty-staged": "pretty-quick --staged --pattern \"**/*.{ts,js,json,html,scss,css}\"",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "gh-prod-deploy": "ng build --output-path ./../docs --base-href /expense-manager/"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.0",
        "@angular/cdk": "^17.3.10",
        "@angular/common": "^17.0.0",
        "@angular/compiler": "^17.0.0",
        "@angular/core": "^17.0.0",
        "@angular/forms": "^17.0.0",
        "@angular/material": "^17.3.10",
        "@angular/platform-browser": "^17.0.0",
        "@angular/platform-browser-dynamic": "^17.0.0",
        "@angular/router": "^17.0.0",
        "@ngrx/effects": "^17.2.0",
        "@ngrx/store": "^17.2.0",
        "@ngrx/store-devtools": "^17.2.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "typescript": "^5.4.5",
        "zone.js": "~0.14.3"
